<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.librarysystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LibrarySystem">
        <activity android:name=".eatit.SearchActivity"/>
        <activity android:name=".eatit.AddBookActivity" />
        <activity android:name=".eatit.AddUserActivity" />
        <activity android:name=".eatit.SearchUsersActivity" />
        <activity android:name=".eatit.LoanUserActivity" />
        <activity android:name=".eatit.SearchLoanedBooksActivity"/>
        <activity android:name=".eatit.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>